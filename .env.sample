# Base API URLs
API_HTTP_BASE_URL=https://api.self-link.com/api/v1
EXPO_PUBLIC_API_BASE_URL=https://api.self-link.com/api/v1
EXPO_PUBLIC_REALTIME_URL=wss://api.self-link.com/ws
# Optional override for health endpoint path
EXPO_PUBLIC_HEALTH_ENDPOINT=/api/v1/health/

# Payment provider base URLs (optional, only if enabled)
EXPO_PUBLIC_IPAY_BASE_URL=
EXPO_PUBLIC_BTCPAY_BASE_URL=

# Stripe publishable key (optional, for IAP/Stripe integrations)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=

# Google Maps API key (required for map features on Android if enabled)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=

# IAP SKUs allowlist (comma-separated)
EXPO_PUBLIC_IAP_SKUS=com.selflink.slc.499,com.selflink.slc.999,com.selflink.slc.1999
