{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "bundler",
    "types": ["react", "react-native", "jest", "node"],
    "baseUrl": ".",
    "paths": {
      "@api/*": ["src/api/*"],
      "@components/*": ["src/components/*"],
      "@store/*": ["src/store/*"],
      "@schemas/*": ["src/types/*"],
      "@screens/*": ["src/screens/*"],
      "@theme": ["src/theme"],
      "@theme/*": ["src/theme/*"],
      "@navigation/*": ["src/navigation/*"],
      "@lib/*": ["src/lib/*"],
      "@realtime/*": ["src/realtime/*"],
      "@utils/*": ["src/utils/*"],
      "@hooks/*": ["src/hooks/*"],
      "@config/*": ["src/config/*"],
      "@services/*": ["src/services/*"],
      "@context/*": ["src/context/*"],
      "@auth/*": ["src/auth/*"]
    }
  }
}
